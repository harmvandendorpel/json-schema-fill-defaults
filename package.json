{
  "name": "json-schema-fill-defaults",
  "version": "2.3.0",
  "description": "Uses JSON Schema to fill in missing required values that have a default value.",
  "main": "dist/bundle.js",
  "scripts": {
    "test:unit": "node test/test.js",
    "test:lint": "eslint src",
    "test": "npm run build && npm run test:lint && npm run test:unit",
    "build": "babel src/index.js --out-file dist/bundle.js",
    "watch": "babel-watch src/index.js --out-file dist/bundle.js",
    "debug": "npm run build && npm run test:unit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harmvandendorpel/json-schema-fill-defaults.git"
  },
  "keywords": [
    "json",
    "schema",
    "defaults"
  ],
  "author": "Harm van den Dorpel <harmvandendorpel@gmail.com>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/harmvandendorpel/json-schema-fill-defaults/issues"
  },
  "homepage": "https://github.com/harmvandendorpel/json-schema-fill-defaults#readme",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-watch": "^2.0.5",
    "is-my-json-valid": "^2.15.0",
    "eslint": "^3.7.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-module-utils": "^2.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0"
  }
}
